{
  "author": "Jerry Zhou",
  "name": "NFCProject",
  "version": "0.0.0",
  "description": "NFC book website",
  "main": "app/app.js",
  "scripts": {
    "dev-server": "webpack-dev-server --config webpack-dev-server.config.js --progress --colors --port 2992 --inline",
    "hot-dev-server": "webpack-dev-server --config webpack-hot-dev-server.config.js --hot --progress --colors --port 2992 --inline",
    "build": "webpack --config webpack-production.config.js --progress --profile --colors",
    "start-dev": "node lib/server-development",
    "start": "node lib/server-production"
  },
  "license": "MIT",
  "dependencies": {
    "async": "^1.3.0",
    "autoprefixer-loader": "^3.0.0",
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-syntax-decorators": "^6.3.13",
    "babel-plugin-transform-decorators": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "body-parser": "^1.12.2",
    "css-loader": "^0.23.0",
    "express": "^4.12.3",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.8.1",
    "history": "^1.17.0",
    "html-loader": "^0.4.0",
    "json-loader": "^0.5.2",
    "less": "^2.5.0",
    "less-loader": "^2.2.0",
    "material-design-icons": "^2.0.0",
    "material-ui": "^0.14.0",
    "material-ui-sass": "^0.7.2",
    "node-sass": "^3.2.0",
    "null-loader": "^0.1.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.6",
    "react-hot-loader": "^1.2.5",
    "react-proxy-loader": "^0.3.4",
    "react-router": "^1.0.0",
    "react-tap-event-plugin": "^0.2.1",
    "sass-loader": "^3.0.1",
    "style-loader": "0.13.0",
    "superagent": "^1.2.0",
    "url-loader": "^0.5.5",
    "uuid": "^2.0.1",
    "webpack": "^1.8.4",
    "webpack-dev-server": "^1.8.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "mocha": true
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "strict": 0,
      "quotes": [
        1,
        "double"
      ],
      "curly": [
        2,
        "multi-line"
      ],
      "space-after-keywords": [
        2,
        "always"
      ],
      "no-use-before-define": [
        2,
        "nofunc"
      ],
      "no-unused-vars": [
        2,
        {
          "vars": "all",
          "args": "none"
        }
      ],
      "no-shadow": 0,
      "no-multi-spaces": [
        1,
        {
          "exceptions": {
            "VariableDeclarator": true
          }
        }
      ],
      "no-mixed-requires": 0,
      "no-underscore-dangle": 0,
      "space-unary-ops": [
        2,
        {
          "words": false,
          "nonwords": false
        }
      ],
      "key-spacing": 0
    }
  }
}
